  "scripts": {
    "prebuild": "rimraf dist",
    "generateTypes": "ts-node src/backk/parser/generateTypescriptFilesFromTypeDefinitionFiles.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
